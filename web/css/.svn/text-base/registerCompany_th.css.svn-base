body {
	overflow-x: hidden;
	min-height:100%;
	padding-bottom: 20px;
	background: url(../img/beauty-bg.png) center repeat-y;
 	background-size:100%;
	background-position: center;
}
.whole-c {
	padding-bottom:20px;
	height: 100%;
}
.main-content{
 	width: 800px;
   margin: 48px auto 40px;
   padding-top:40px;
   height: 100%;
}
.step-list {
	background-color: rgba(255,255,255,.65);
	color:#363434;
}
.step-list li {
	text-align:center;
	display: inline-block;
	width:33%;
	padding:8px 15px;
	position: relative;
}
.step-list li span {
	padding:6px 0;
}
.step-active {
	border-bottom: 4px solid #ffba01;
}
.step-list li i {
	display:inline-block;
	position: absolute;
	right:15px;
	top:0;
	height:36px;
	line-height: 36px;
	font-size:20px;
}
.s-one {
	width:240px;
	margin:10px auto 20px;
	text-align: center;
	/* display: none; */
}
.s-one input {
	display: block;
	background-color: rgba(255,255,255,.9);
	border:1px solid transparent;
	outline:none;
	width:100%;
	margin-bottom: 16px;
	padding:6px 10px;
}
.renzheng {
	display: block;
	overflow: hidden;
}
.renzheng input,
.renzheng button {
	width:50%;
	float:left;
}
.get-code2 {
	height:32px;
	background-color: #ffba01;
	color:#fff;
	border:none;
}
.sure-r {
	width:100%;
	height:32px;
	background-color: #ffba01;
	color:#fff;
	border:none;
}
.s-two {
	width:600px;
	margin:20px auto 20px;
 	display: none; 
	background-color: rgba(255,255,255,.8);
	padding:10px 40px 20px;
}
.s-two .t-head {
	text-align: center;
	color:#e6041e;
	font-size:18px;
	font-weight: 500;
}
.t-head .fa-bell-o {
	margin-right: 10px;
}
.s-two > div {
	padding:5px 0;
	border-bottom: 1px solid #999;
	margin-bottom: 4px;
}
.s-two label {
	font-weight: normal;
	display: inline-block;
	width:100px;
	text-align: right;
}
.s-two input {
	border:none;
	background-color:transparent!important;
	padding:3px 6px;
}
#license,
#brand-input {
    position: absolute;
    right: -400px;
    top: 0;
}
.rz-status {
	width:60px;
}
.brand {
	width:30px;
	height:30px;
}
.tip-p {
	/* padding-left:100px; */
}
.img-wrap {
	margin-left:100px;
	margin-top:16px;
}
.scimg {
	display:inline-block;
	vertical-align: top;
	position: relative;
}
.scimg img{
	width:100px;
	height:100px;
}
.scimg i{
	position: absolute;
	top:0;
	right:0;
	color:red;
	font-size:16px;
	display: none;
}
.scimg i:hover {
	cursor: pointer;
}
.up-img {
	display:inline-block;
	border:1px dashed #999; 
	text-align:center;
	position:relative;
	width:100px;
	height:100px;
	margin-right: 16px;
}
.fa-plus {
	font-size:30px;
	color:#979b9b;
	line-height:100px!important;
	height:100px!important;
	z-index:5;
	font-weight:normal;
}
.gs-div label{
	vertical-align: top;
}
.gs-desc {
	font-size: 14px;
    text-indent: 34px;
    font-family: "宋体";
    display: inline-block;
    width: 80%;
    border: none;
    background-color: #a9aaaa;
    outline: none;
    overflow: hidden;
	min-height:100px;
    padding:6px 10px;
}
.save-c {
	background-color: #ffba01;
	color:#fff;
	margin-top:14px;
}
.s-three {
	width:600px;
	margin:40px auto 20px;
	text-align: center;
	display:none;
	background-color: rgba(255,255,255,.65);
	padding:20px 40px 40px;
	font-family: "微软雅黑";
}
.s-three .t-head{
	font-size:18px;
}
.t-wrap {
	position: relative;
	padding-bottom: 20px;
}
.t-wrap .jump {
	position: absolute;
	left:58%;
	margin-top:10px;
}
.t-wrap .jump span {
	color:#f20c0c;
	margin-right:4px;
}
/* dropdown */
.dropdown > button {
	display: inline-block;
    width: 170px;
    text-align: left;
    font-size: 14px;
    height: 30px;
    line-height: 20px;
    border:1px solid rgba(0, 0, 0, 0.15);
    padding:4px 8px;
    color:#333;
    box-shadow: 10px 3px -2px  rgba(0, 0, 0, 0.5)!important;
	-webkit-box-shadow: 10px 3px -2px rgba(0, 0, 0, 0.5)!important;
 	-moz-box-shadow: 10px 3px -2px  rgba(0, 0, 0, 0.5)!important;
 	position: relative;
 	background-color: #fff!important;
}
.chosen-item {
	line-height: 20px;
	display: inline-block;
	/* color:rgba(102,102,102); */
}
.dropdown > button:hover {
	color:#333;
}
.dropdown .caret {
	float:right;
	margin-top:10px;
}
.dropdown ul{
    border-radius: 2px;
    outline: none;
    box-shadow: none;
    overflow:auto;
    background-color: #5d5d5d;
    height:140px;
}
.dropdown-menu li a{
	padding:6px 20px;
	color:#fff;
	text-align: center;
}
.dropdown-menu li a:hover {
	background-color: #f6f5fb!important;
  	text-decoration: none;
  	outline: 0;
  	color:#ffba01;
  	background-image: -moz-linear-gradient(top, #f6f5fb, #f6f5fb);
	background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#f6f5fb), to(#f6f5fb));
	background-image: -webkit-linear-gradient(top, #f6f5fb, #f6f5fb);
	background-image: -o-linear-gradient(top, #f6f5fb, #f6f5fb);
	background-image: linear-gradient(to bottom, #f6f5fb, #f6f5fb);
	background-repeat: repeat-x;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#f6f5fb', endColorstr='#f6f5fb', GradientType=0);
}
.s-wrap {
	width:300px;
	margin: 0 auto;
	padding:10px 30px 30px;
	background-color: rgba(255,255,255,.65);
	margin-top: 40px;
}